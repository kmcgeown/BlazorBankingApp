{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "Override": {
      "System": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "../logs.store-.json",
          "rollingInternval": "Day",
          "formatter": "Serilog.Formatting.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  },
  "LokiServer": "http://localhost:3100",
  "AllowedHosts": "*"
}
